@model ApiMvcTester.Models.Student

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <div>
        <form action="/Home/Delete" method="post" class="form-control-static">

            <input type="hidden" name="Id" value="@Model.Id" />
            <textarea rows="1" cols="20" name="Name">@Model.Name</textarea>
            <textarea rows="1" cols="30" name="email">@Model.email</textarea>
            <textarea rows="1" cols="20" name="Degree">@Model.Degree</textarea>
            <input type="submit" name="Confirm" value="Confirm" />
        </form>
    </div>
</body>
</html>
